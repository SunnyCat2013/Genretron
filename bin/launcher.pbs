#!/usr/bin/env bash
#PBS -S /bin/bash

function require_vars {
    for var in "$@"
    do
        if [ -z "$var" ]
            echo
            then exit 1
        fi
    done
}

. ~/.bashrc

. $GENRETRON_PATH/env/bin/activate

COMMAND="$GENRETRON_PATH/bin/jobman sql -n $CONSECUTIVE_JOBS $DB_TABLE $GENRETRON_PATH/results"

echo $COMMAND
$COMMAND
